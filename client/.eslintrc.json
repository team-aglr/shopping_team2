{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "rules": {
    "react/react-in-jsx-scope": 0
  },
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}